        <activity
            android:name="dehaagsehogeschool.digiveilig.HelpActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="dehaagsehogeschool.digiveilig.toets.DigiveiligToetsAnnouncementActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="dehaagsehogeschool.digiveilig.toets.DigiveiligToetsEndActivity"
