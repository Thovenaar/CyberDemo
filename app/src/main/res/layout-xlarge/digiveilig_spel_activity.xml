<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/home_background"
    tools:layout_editor_absoluteY="25dp">

    <ImageView
        android:id="@+id/digiveilig_spel_text"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:contentDescription="@string/digiveilig_spel_text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/digiveilig_spel_text_v2" />

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="0dp"
        android:layout_height="130dp"
        android:layout_marginEnd="80dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="16dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/digiveilig_spel_star_score">

        <Button
            android:id="@+id/level_button_1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/digiveilig_spel_0_star"
            android:fontFamily="@font/bree_serif"
            android:onClick="startLevel1"
            android:text="@string/Digiveilig_spel_level_number"
            android:textColor="@color/white"
            android:textSize="40sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toStartOf="@+id/imageView18"
            tools:layout_editor_absoluteY="290dp" />

        <Button
            android:id="@+id/level_button_2"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/digiveilig_spel_lock"
            app:layout_constraintEnd_toEndOf="parent"
            tools:layout_editor_absoluteY="217dp" />

        <Button
            android:id="@+id/level_button_3"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/digiveilig_spel_lock"
            app:layout_constraintEnd_toStartOf="@+id/imageView19"
            tools:layout_editor_absoluteY="217dp" />

        <Button
            android:id="@+id/level_button_4"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/digiveilig_spel_lock"
            app:layout_constraintEnd_toStartOf="@+id/imageView21"
            tools:layout_editor_absoluteY="217dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="0dp"
        android:layout_height="130dp"
        android:layout_marginEnd="80dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout1">

        <Button
            android:id="@+id/level_button_5"
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/digiveilig_spel_lock"
            app:layout_constraintEnd_toStartOf="@+id/imageView18"
            tools:layout_editor_absoluteY="290dp" />

        <Button
            android:id="@+id/level_button_6"
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/digiveilig_spel_lock"
            app:layout_constraintEnd_toEndOf="parent"
            tools:layout_editor_absoluteY="217dp" />

        <Button
            android:id="@+id/level_button_7"
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/digiveilig_spel_lock"
            app:layout_constraintEnd_toStartOf="@+id/imageView19"
            tools:layout_editor_absoluteY="217dp" />

        <Button
            android:id="@+id/level_button_8"
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/digiveilig_spel_lock"
            app:layout_constraintEnd_toStartOf="@+id/imageView21"
            tools:layout_editor_absoluteY="217dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="0dp"
        android:layout_height="130dp"
        android:layout_marginEnd="80dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2">

        <Button
            android:id="@+id/level_button_9"
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/digiveilig_spel_lock"
            app:layout_constraintEnd_toStartOf="@+id/imageView18"
            tools:layout_editor_absoluteY="290dp" />

        <Button
            android:id="@+id/level_button_10"
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/digiveilig_spel_lock"
            app:layout_constraintEnd_toEndOf="parent"
            tools:layout_editor_absoluteY="217dp" />

        <Button
            android:id="@+id/level_button_11"
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/digiveilig_spel_lock"
            app:layout_constraintEnd_toStartOf="@+id/imageView19"
            tools:layout_editor_absoluteY="217dp" />

        <Button
            android:id="@+id/level_button_12"
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/digiveilig_spel_lock"
            app:layout_constraintEnd_toStartOf="@+id/imageView21"
            tools:layout_editor_absoluteY="217dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="0dp"
        android:layout_height="130dp"
        android:layout_marginEnd="80dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3">

        <Button
            android:id="@+id/level_button_13"
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/digiveilig_spel_lock"
            app:layout_constraintEnd_toStartOf="@+id/imageView18"
            tools:layout_editor_absoluteY="290dp" />

        <Button
            android:id="@+id/level_button_14"
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/digiveilig_spel_lock"
            app:layout_constraintEnd_toEndOf="parent"
            tools:layout_editor_absoluteY="217dp" />

        <Button
            android:id="@+id/level_button_15"
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/digiveilig_spel_lock"
            app:layout_constraintEnd_toStartOf="@+id/imageView19"
            tools:layout_editor_absoluteY="217dp" />

        <Button
            android:id="@+id/level_button_16"
            android:layout_width="10dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/digiveilig_spel_lock"
            app:layout_constraintEnd_toStartOf="@+id/imageView21"
            tools:layout_editor_absoluteY="217dp" />
    </LinearLayout>

    <TextView
        android:id="@+id/digiveilig_spel_star_score"
        android:layout_width="wrap_content"
        android:layout_height="88dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/bree_serif"
        android:text="@string/home_sterren"
        android:textColor="@color/white"
        android:textSize="50sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:contentDescription="@string/digiveilig_spel_star"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/digiveilig_spel_text"
        app:srcCompat="@drawable/home_star" />

</android.support.constraint.ConstraintLayout>